app_identifier(process.env.package_name) # The bundle identifier of your app
apple_id(process.env.email) # Your Apple email address

itc_team_id(process.env.itc_team_id) # App Store Connect Team ID
team_id(process.env.team_id) # Developer Portal Team ID

